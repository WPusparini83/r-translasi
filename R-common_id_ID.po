msgstr "nilai pada argumen '%s' harus berupa bilangan berhingga"

#: src/main/seq.c:855
msgid "'length.out' must be a non-negative number"
msgstr "nilai pada argumen 'length.out' harus bernilai positif"

#: src/main/seq.c:866 src/main/seq.c:873 src/main/seq.c:881
#, c-format
msgid "'%s' must be of length 1"
msgstr "nilai pada argumen '%s' harus berukuran 1 (terdiri atas satu elemen saja)"

#: src/main/seq.c:894
msgid "invalid '(to - from)/by'"
msgstr "periksa kembali nilai pada argumen 'from', 'to', dan 'by' (karena hasil dari operasi '(to - from)/by' tidaklah valid)"

#: src/main/seq.c:906
msgid "'by' argument is much too small"
msgstr "nilai pada argumen 'by' terlalu kecil"

#: src/main/seq.c:908
msgid "wrong sign in 'by' argument"
msgstr "nilai pada argumen 'by' memiliki penanda positif (+) atau negatif (-) yang salah"

#: src/main/seq.c:1074 src/main/seq.c:1081
msgid "argument must be coercible to non-negative integer"
msgstr "nilai pada argumen harus dapat diubah menjadi bilangan bulat non-negatif"

#: src/main/seq.c:1101
msgid "'lengths' is not of mode integer"
msgstr "nilai pada argumen 'lengths' harus merupakan bilangan bulat"

#: src/main/seq.c:1104
msgid "'from' is not of mode integer"
msgstr "nilai pada argumen 'from' harus merupakan bilangan bulat"

#: src/main/seq.c:1107
msgid "'by' is not of mode integer"
msgstr "nilai pada argumen 'by' harus merupakan bilangan bulat"

#: src/main/seq.c:1114
